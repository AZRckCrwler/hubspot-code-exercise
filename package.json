{
  "name": "hubspot-code-exercise",
  "version": "1.1.1",
  "description": "HubSpot Code Exercise",
  "scripts": {
    "dev": "vite",
    "start": "vite build && vite preview --port 1234",
    "build": "run-p type-check build-only",
    "lint": "run-p js-lint css-lint type-check",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "js-lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "css-lint": "stylelint '**/*.{vue,css}' --fix --ignore-path .gitignore"
  },
  "author": "HubSpot Applicant - James T. Wilson",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/node": "^16.11.36",
    "@types/pluralize": "^0.0.29",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.18.0",
    "eslint-plugin-vue": "^9.1.1",
    "npm-run-all": "^4.1.5",
    "pluralize": "^8.0.0",
    "postcss": "^8.4.14",
    "postcss-nested": "^5.0.6",
    "postcss-rem": "^2.0.2",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "typescript": "~4.7.4",
    "vite": "^2.9.12",
    "vue-tsc": "^0.37.9"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
